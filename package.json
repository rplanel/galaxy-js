{
  "name": "galaxy-js",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.9.0",
  "description": "_description_",
  "author": "RÃ©mi Planel <rplanel@pasteur.fr>",
  "license": "MIT",
  "homepage": "https://github.com/rplanel/galaxy-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rplanel/galaxy-js.git"
  },
  "bugs": "https://github.com/rplanel/galaxy-js/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.7.4",
    "bumpp": "^9.6.1",
    "eslint": "^9.11.1",
    "esno": "^4.8.0",
    "h3": "^1.12.0",
    "lint-staged": "^15.2.10",
    "ofetch": "1.3.4",
    "pnpm": "^9.11.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
